{
  "name": "@svelte-up/cli",
  "version": "0.0.1",
  "description": "Cybernetically enhanced cloud deployments.",
  "homepage": "https://get.svelteup.today",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "https://github.com/medelman17/svelte-up",
  "author": "Michael Edelman <mike.edelman@gmail.com>",
  "maintainers": [],
  "bugs": "https://github.com/medelman17/svelte-up/issues",
  "license": "Apache-2.0",
  "bin": {
    "sup": "build/index.js"
  },
  "scripts": {
    "dev": "DEV=true node helpers/build.js",
    "build": "node helpers/build.js",
    "format": "prettier --loglevel warn --write \"**/*.{ts,js,json,yaml}\"",
    "format:check": "prettier --loglevel warn --check \"**/*.{ts,js,json,yaml}\"",
    "lint": "eslint --cache --fix --ext .ts .",
    "lint-ci": "eslint --ext .ts .",
    "precommit": "lint-staged",
    "prepublishOnly": "pnpm run build",
    "package:linux": "pkg . --no-bytecode --targets node14-linux-x64 --output dist/linux/sup --compress GZip",
    "package:macos": "pkg . --no-bytecode --targets node14-macos-x64 --output dist/macos/sup --compress GZip",
    "test": "ava",
    "test:ci": "nyc ava --fail-fast",
    "test:watch": "ava --watch"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "@types/resolve": "^1.20.1",
    "@types/rimraf": "^3.0.2",
    "@types/shell-quote": "^1.7.1",
    "@types/tar": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "esbuild": "^0.13.7",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^25.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "fast-deep-equal": "^3.1.3",
    "get-port": "^6.0.0",
    "glob": "^7.2.0",
    "global-dirs": "^3.0.0",
    "indent-string": "^5.0.0",
    "is-installed-globally": "^0.4.0",
    "jest": "^27.2.5",
    "line-replace": "^2.0.1",
    "log-update": "^4.0.0",
    "make-dir": "^3.1.0",
    "node-fetch": "^3.0.0",
    "open": "^8.3.0",
    "pkg": "^5.3.3",
    "pkg-up": "^3.1.0",
    "prettier": "^2.4.1",
    "replace-string": "^4.0.0",
    "resolve-pkg": "^2.0.0",
    "rimraf": "^3.0.2",
    "strip-ansi": "^7.0.1",
    "tempy": "^2.0.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^8.2.0",
    "execa": "^5.1.1",
    "figlet": "^1.5.2",
    "fs-jetpack": "^4.2.0",
    "p-map": "^5.1.0",
    "p-retry": "^4.6.1",
    "unique-names-generator": "^4.6.0"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "outputPath": "dist"
  },
  "files": [
    "/assets",
    "/build"
  ],
  "engines": {
    "node": ">=14.18.1"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
